{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js",
      "headers": {
        "Access-Control-Allow-Origin": [
          "http://localhost:3000",
          "https://development-filkom-apps-fe.vercel.app",
          "http://localhost:5173"
        ]
      }
    }
  ]
}
